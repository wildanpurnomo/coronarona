<!-- Modal Add Data Donasi-->
<div id="form-add" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Tambah Data Donasi</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="formInputDonasi">
                    <div class="form-group">
                        <label for="modalInputName">Nama</label>
                        <input type="text" class="form-control" id="modalInputName" name="donasiName" placeholder="Nama Donasi">
                    </div>

                    <div class="form-group">
                        <label for="modalInputDesc">Deskripsi</label>
                        <input type="text" class="form-control" id="modalInputDesc" name="donasiDesc"
                            placeholder="Deskripsi Donasi">
                    </div>

                    <div class="form-group">
                        <label for="modalInputCP">Contact Person</label>
                        <input type="text" class="form-control" id="modalInputCP" name="donasiCP"
                            placeholder="CP Donasi">
                    </div>

                    <div class="form-group">
                        <label for="modalInputURL">Url</label>
                        <input type="text" class="form-control" id="modalInputURL" name="donasiURL"
                            placeholder="Link Penunjang">
                    </div>

                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>